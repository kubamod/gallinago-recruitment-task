{
    "name": "kubamod/freshsalesapiclient",
    "type": "library",
    "description": "",
    "keywords": [
        "kubamod",
        "FreshsalesApiClient"
    ],
    "homepage": "https://github.com/kubamod/FreshsalesApiClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub",
            "email": "kubam1414@hotmail.com",
            "homepage": "https://github.com/kubamod",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "kubamod\\FreshsalesApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kubamod\\FreshsalesApiClient\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
